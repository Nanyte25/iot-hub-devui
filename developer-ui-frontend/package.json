{
  "name": "BoschIoTHubDeveloperUI",
  "version": "0.0.1",
  "description": "A React Redux Application for accessing the core functionalities of the Bosch IoT Hub",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
    "prod": "cross-env NODE_ENV=production PUBLIC_PATH=/hubdev/ui/ webpack -p",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 9001  -c .storybook",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "watch": "cross-env NODE_ENV=test jest --watch --verbose",
    "generate-disclosure": "cross-env NODE_ENV=production node generate-disclosure",
    "update-snapshots": "cross-env NODE_ENV=test jest -u --verbose"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|ico|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(s?css|sass|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Tim Weise",
  "license": "SEE LICENSE IN LICENSE.TXT",
  "devDependencies": {
    "@storybook/addon-info": "^3.2.18",
    "@storybook/addon-options": "^3.2.18",
    "@storybook/addon-storyshots": "^3.2.18",
    "@storybook/addons": "^3.2.18",
    "@storybook/react": "^3.2.18",
    "autoprefixer": "^7.2.5",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.0.3",
    "node-sass": "^4.5.2",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.0.10",
    "react-immutable-proptypes": "^2.1.0",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.3.0",
    "redux-testkit": "^1.0.6",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "html-react-parser": "^0.4.0",
    "immutable": "^3.8.1",
    "jssha": "^2.3.1",
    "lodash": "^4.17.5",
    "lodash.range": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "pretty-checkbox": "^3.0.3",
    "prismjs": "^1.8.1",
    "react": "^16.3.2",
    "react-click-outside": "^3.0.0",
    "react-dom": "^16.3.2",
    "react-json-edit": "^0.3.1",
    "react-measure": "2.0.2",
    "react-modal": "^3.1.8",
    "react-motion": "^0.5.2",
    "react-notification-system": "^0.2.16",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-last-location": "^1.1.0",
    "react-router-transition": "^1.2.1",
    "react-tooltip": "^3.3.0",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "typed.js": "^2.0.7",
    "uuid": "^3.2.1",
    "velocity-react": "^1.3.3",
    "vertx3-eventbus-client": "^3.4.2"
  }
}
